<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension
         point="org.knime.workbench.repository.nodes">
      <node
            category-path="/community/dbase"
            factory-class="ch.res_ear.samthiriot.knime.dbase.readDromDBase.DBaseReaderNodeFactory"
            id="ch.res_ear.samthiriot.knime.dbase.readDromDBase.DBaseReaderNodeFactory"/>
   </extension>
   <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="read data stored in DBase file format"
            icon="src/ch/res_ear/samthiriot/knime/dbase/readDromDBase/dbasereader-base.png"
            level-id="dbase"
            locked="false"
            name="DBase"
            path="/community">
      </category>
   </extension>
   
</plugin>
